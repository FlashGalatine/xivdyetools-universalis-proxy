{
  "name": "xivdyetools-universalis-proxy",
  "version": "1.2.2",
  "description": "Cloudflare Worker proxy for Universalis API with CORS support",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "dev:local": "wrangler dev --local",
    "deploy": "wrangler deploy",
    "deploy:production": "wrangler deploy --env production",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "xivdyetools",
    "universalis",
    "cors",
    "proxy",
    "cloudflare-workers"
  ],
  "author": "FlashGalatine",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^4.0.16",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "wrangler": "^4.55.0"
  },
  "dependencies": {
    "hono": "^4.6.14"
  }
}
